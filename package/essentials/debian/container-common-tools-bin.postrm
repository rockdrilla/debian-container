#!/bin/sh

set -e

case "$1" in
remove)
	for b in uptime ; do
		for f in "/usr/bin/$b" "/bin/$b" ; do
			[ -e "$f.real" ] || continue
			! [ -h "$f" ] || rm -fv "$f"
			dpkg-query -S "$f" 2>/dev/null || continue
			dpkg-divert --package '#PACKAGE#' --rename --remove "$f"
		done
	done ; unset b f
;;
esac

#DEBHELPER#

exit 0
